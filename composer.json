{
    "name": "erikgreasy/superfaktura-client",
    "description": "HTTP client for SuperFaktura REST api",
    "keywords": [
        "superfaktura rest api",
        "http client"
    ],
    "homepage": "https://github.com/erikgreasy/superfaktura-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Erik Masny",
            "email": "erik.masny@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Erikgreasy\\Superfaktura\\": "src"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
